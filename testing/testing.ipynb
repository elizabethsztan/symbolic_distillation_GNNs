    "def get_edge_index(num_nodes): #edge index for fully connected graph\n",
    "    idx = torch.arange(num_nodes)\n",
    "    edge_index = torch.cartesian_prod(idx, idx)\n",
    "    edge_index = edge_index[edge_index[:, 0] != edge_index[:, 1]]\n",
    "    return edge_index.t()\n",
    "\n",
